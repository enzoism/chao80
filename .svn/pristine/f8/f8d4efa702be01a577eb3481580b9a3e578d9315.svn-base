<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="account" extends="struts-default">
		<action name="account_*" class="cn.smartcandy.application.a.account.AccountAction" method="{1}">
			<!--用户注册成功 -->
			<result name="success">/success.html</result>
			<result name="addCategarySuccess" type="redirect">/admin/adminHomeCategary.html</result>
			<result name="registerSuccess" type="redirect">/index.html</result>
			<result name="loginSuccess" type="redirect">/admin/index.html</result>
			<result name="loginOut" type="redirect">/index.html</result>
			<result name="loginError" type="redirect">/index.html</result>
		</action>
	</package>
</struts>