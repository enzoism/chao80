.pages{
  padding-top:100px;
  height:30px;
  padding-bottom:30px;
  position: relative;
  text-align:center;
}
.pages:before{
  width:100%;
  height:2px;
  background: #313131;
  content: '';
  position: absolute;
  left:0;
  top:115px;
  z-index: 0;
}
.pages #page{
  width:auto;
  height:30px;
  background:#0d0d0d;
  display: inline-block;
  position: relative;
  z-index: 2;
  padding:0 15px;
}
.pages #page a,
.pages #page span{
  float: left;
  font-size:12px;
  width:36px;
  text-align:center;
  line-height:30px;
  cursor: pointer;
}
.pages #page span{
  font-size:14px;
  color: #ffffff;
}

